mn = raw_input()
n,m = map(int,mn.split())

n_fac=1
for i in range(n):
    n_fac=n_fac * (i + 1)

m_fac=1
for i in range(m):
    m_fac=m_fac * (i + 1)


diff_fac=1
for i in range(n - m):
    diff_fac=diff_fac * (i + 1)

P=n_fac/diff_fac
C=n_fac/(m_fac * diff_fac)
print("the result of P:" + str(P))
print("the result of C:" + str(C))
